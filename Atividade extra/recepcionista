-- Testa a fun��o sys_xmlgen
-- SELECT SYS_XMLGEN(*) as XML FROM tb_paciente;

set serveroutput on;

DECLARE
    ctx dbms_xmlgen.ctxhandle;
    answer clob;
BEGIN
    -- criar um novo contexto para a consulta SQL
    ctx := dbms_xmlgen.newContext('SELECT r.id, r.nome, r.telefone, r.endereco FROM tb_recepcionista r');
    -- personalizar as tags raiz e de entidade
    DBMS_XMLGEN.setRowsetTag(ctx, 'CLINICA');
    DBMS_XMLGEN.setRowTag(ctx,'RECEPCIONISTA');
    -- gerar um valor CLOB como resultado
    answer := dbms_xmlgen.getXML(ctx);
    -- dar sa�da do resultado
    dbms_output.put_line(answer);
    
    -- fechar o contexto
    dbms_xmlgen.closeContext(ctx);
END;
